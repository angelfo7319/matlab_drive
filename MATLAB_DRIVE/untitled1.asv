function [V, A] = untitled1 (a,b)
% a)
V = 0.25*pi^2*(a+b).*(b-a).^2; % Formula to calculate volume.
A = pi^2*(b.^2-a.^2);          % Formula to calculate area.

% b)

a = [0.25:0.01:4];
b = a + 2;
[V,A] = untitled1.m(a,b);
plot(a,V), xlabel('a(in.)'), ylabel('V(cu.in.)')
plot(a,A), xlabel('a(in.)'), ylabel('V(cu.in.)')
end

